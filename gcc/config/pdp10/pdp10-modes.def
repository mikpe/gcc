/* Character modes */
FRACTIONAL_INT_MODE (Q6I, 6, 1);
FRACTIONAL_INT_MODE (Q7I, 7, 1);
FRACTIONAL_INT_MODE (Q8I, 8, 1);
FRACTIONAL_INT_MODE (Q9I, 9, 1);

/* Pointer modes */
#define BITS_PER_UNIT 9
make_partial_integer_mode ("SI", "Q6P", BITS_PER_UNIT*4, __FILE__, __LINE__);
make_partial_integer_mode ("SI", "Q7P", BITS_PER_UNIT*4, __FILE__, __LINE__);
make_partial_integer_mode ("SI", "Q8P", BITS_PER_UNIT*4, __FILE__, __LINE__);
make_partial_integer_mode ("SI", "Q9P", BITS_PER_UNIT*4, __FILE__, __LINE__);
make_partial_integer_mode ("SI", "HP", BITS_PER_UNIT*4, __FILE__, __LINE__);
make_partial_integer_mode ("SI", "WP", BITS_PER_UNIT*4, __FILE__, __LINE__);

